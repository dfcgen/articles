\[
1-y^{2}=\ecn[2]{\frac{u}{M}}{\lambda}=A^{2}\frac{\prod\limits _{\mu=1,3,5,\ldots}^{4n-1}\left[x-\esn{\mu\frac{K}{n}}{k}\right]}{\prod\limits _{\nu=2,4,6,\ldots}^{n-1}\left[1-k^{2}\esn[2]{\nu\frac{K}{n}}{k}x^{2}\right]^{2}}
\]

\end_inset


\end_layout

